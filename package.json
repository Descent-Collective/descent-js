{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "descent-js",
  "author": "Njoku Emmanuel",
  "description": "A Typescript library for interacting with the Descent Protocol",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Descent-Collective/descent-js"
  },
  "keywords": [
    "xNGN",
    "Web3",
    "Lending",
    "SDK"
  ],
  "module": "dist/descent-js.esm.js",
  "size-limit": [
    {
      "path": "dist/descent-js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/descent-js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@size-limit/preset-small-lib": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "rollup": "^3.29.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "size-limit": "^9.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ethers": "^6.7.1"
  }
